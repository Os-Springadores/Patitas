//NOME DO PACIENTE DA TABELA CONSULTA ATRAVES DO PACIENTE_ID

SELECT P.NOME
FROM TB_CONSULTA C
JOIN TB_PACIENTE P ON C.PACIENTE_ID = P.ID
WHERE C.ID = 2;

//HISTORICO MEDICO PARA CADA ANIMAL, VISITAS E TRATAMENTOS REALIZADOS
SELECT P.ID, P.NOME, C.SERVICO, C.TIPO_SERVICO, C.PRECO, H.HORARIOS_DISPONIVEIS
FROM TB_CONSULTA C
JOIN TB_PACIENTE P ON C.PACIENTE_ID = P.ID
JOIN TB_HORARIOS_DISPONIVEIS H ON C.HORARIOS_DISPONIVEIS_ID = H.ID
WHERE P.ID = 1;